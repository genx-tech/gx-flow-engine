{
  "name": "@genx/wfe",
  "version": "1.0.0",
  "description": "Gen-X workflow engine",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bulid:clean": "shx rm -rf build",
    "build:server:prod": "npm run bulid:clean && cross-env NODE_ENV=production babel src -d build --copy-files && shx rm -rf build/**/__test__",
    "build:server": "npm run bulid:clean && cross-env NODE_ENV=development babel src -d build --copy-files && shx rm -rf build/**/__test__",
    "build": "npm run build:server && lerna run --parallel build",
    "build:prod": "npm run build:server:prod && lerna run --parallel build:prod",
    "start:prod": "cross-env NODE_ENV=production node build/index.js",
    "start": "cross-env NODE_RT=babel babel-node src/index.js",
    "start:dev": "cross-env NODE_RT=babel nodemon --exec babel-node src/index.js",
    "babel": "cross-env NODE_RT=babel NODE_ENV=development babel-node"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@genx/algorithm": "git+https://github.com/genx-tech/gx-algorithm.git",
    "@genx/app": "git+https://github.com/genx-tech/gx-app.git",
    "@genx/data": "git+https://github.com/genx-tech/gx-data.git",
    "@genx/server": "git+https://github.com/genx-tech/gx-server.git",
    "amqplib": "^0.5.5",
    "fast-csv": "^3.4.0",
    "markdown-it": "^10.0.0",
    "mingo": "^2.4.0",
    "mongodb": "^3.3.5",
    "node-cron": "^2.0.3",
    "puppeteer": "^2.0.0",
    "rk-utils": "^2.2.6",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/plugin-proposal-partial-application": "^7.7.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-plugin-contract": "^1.3.3",
    "babel-plugin-source-map-support": "^2.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.1",
    "jsdoc": "^3.6.3",
    "lerna": "^3.15.0",
    "link-parent-bin": "^1.0.2",
    "mocha": "^6.2.0",
    "nodemon": "^2.0.1",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "shx": "^0.3.2",
    "supertest": "^4.0.2"
  }
}
